{
  "name": "typescript-template",
  "version": "0.0.1",
  "description": "A starter template for creating TypeScript projects.",
  "packageManager": "yarn@1.22.22",
  "main": "src/index.ts",
  "author": "Brace Sproul",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.test\\.ts$ --testPathIgnorePatterns=\\.int\\.test\\.ts$",
    "test:int": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.int\\.test\\.ts$",
    "test:single": "NODE_OPTIONS=--experimental-vm-modules yarn run jest --config jest.config.js --testTimeout 100000",
    "format": "prettier --write .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.13",
    "@langchain/community": "^0.3.30",
    "@langchain/core": "^0.3.39",
    "@langchain/langgraph": "^0.2.46",
    "@langchain/openai": "^0.4.4",
    "langchain": "^0.3.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@tsconfig/recommended": "^1.0.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "dotenv": "^16.4.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  }
}
